{
  "name": "react-electron",
  "private": true,
  "version": "0.0.0",
  "main": "electron.js",
  "scripts": {
    "dev:web": "vite",
    "build:web": "tsc && vite build",
    "dev": "concurrently \"vite\" \"wait-on localhost:5066\" \"cross-env NODE_ENV=development electron . \" ",
    "build": "tsc && vite build && cross-env NODE_ENV=production electron-builder",
    "build:ele": "cross-env NODE_ENV=production electron-builder --win --x64"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "antd": "4.17.1",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "electron-reload": "2.0.0-alpha.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.my-test.my-test",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2022 Wegi8"
  },
  "browser": [
    "> 5%"
  ]
}